{
  "extends": [],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "no-console": false,
    "interface-name": [true, "never-prefix"],
    "noImplicitAny": false,
    "member-ordering": [false, {"order": "fields-first"}]
  }
}
